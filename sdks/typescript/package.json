{
  "name": "@120356aa/pulsekit-sdk",
  "version": "1.0.1",
  "description": "PulseKit SDK for TypeScript/JavaScript - Error tracking and event monitoring",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "lint": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "pulsekit",
    "error-tracking",
    "monitoring",
    "observability",
    "logging",
    "events",
    "typescript",
    "sdk"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/pulsekit/pulsekit"
  },
  "homepage": "https://github.com/pulsekit/pulsekit#readme",
  "bugs": {
    "url": "https://github.com/pulsekit/pulsekit/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.3.0"
  }
}

